{
    "name": "fb",
    "api_root": "https://graph.facebook.com",
    "resources": {
        "/:id": {
            "methods": [ "get" ],
            "params": [
                {
                    "q": "optional"
                },
                {
                    "access_token": "required"
                }
            ]
        },
        "/me": {
            "methods": [ "get" ],
            "params": [
                {
                    "fields": "optional"
                },
                {
                    "access_token": "required"
                }
            ]
        }
    }
}